      "$ref": "#/definitions/CsvJWE"
    }
  },
  "required": ["csv"],
  "definitions": {
    "CsvJWE": {
      "type": "object",
      "properties": {
        "protected": { "type": "string" },
        "iv": { "type": "string" },
        "ciphertext": { "type": "string" },
        "tag": { "type": "string" },
        "aad": { "type": "string" },
        "recipients": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "header": { "type": "object" },
              "encrypted_key": { "type": "string" }
            },
            "required": ["header", "encrypted_key"]
          }
        }
      },
      "required": ["protected", "iv", "ciphertext", "tag"]
    }
  }
